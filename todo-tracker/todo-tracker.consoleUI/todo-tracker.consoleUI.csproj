<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>todo_tracker.consoleUI</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ConsoleManager\" />
    <None Remove="ProgramManager\" />
    <None Remove="Ninject" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="ConsoleManager\" />
    <Folder Include="ProgramManager\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Ninject" Version="3.3.6" />
  </ItemGroup>
</Project>
